{
  "name": "zenith-pm-server",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx src/scripts/startWorker.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ api/",
    "format": "prettier --write \"src/**/*.ts\" \"api/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"api/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Zenith PM Server - Express + TypeScript API",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@supabase/supabase-js": "^2.89.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "pino": "^10.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^5.5.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  }
}
